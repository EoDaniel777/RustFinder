# RustFinder Environment Configuration
# Copy this file to .env and fill in your API keys

# VirusTotal API Keys (comma separated for multiple keys)
VIRUSTOTAL_API_KEYS=your_virustotal_key1,your_virustotal_key2

# SecurityTrails API Keys
SECURITYTRAILS_API_KEYS=your_securitytrails_key1,your_securitytrails_key2

# Shodan API Keys
SHODAN_API_KEYS=your_shodan_key1,your_shodan_key2

# Chaos (ProjectDiscovery) API Keys
CHAOS_API_KEYS=your_chaos_key1,your_chaos_key2

# GitHub API Keys (Personal Access Tokens)
GITHUB_API_KEYS=ghp_your_github_token1,ghp_your_github_token2

# Netlas API Keys
NETLAS_API_KEYS=your_netlas_key1,your_netlas_key2

# Output Configuration
OUTPUT_FILE=results/subdomains.txt
OUTPUT_FORMAT=text # Options: text, json, csv

# Resolver Configuration
RESOLVER_ENABLED=true
RESOLVER_THREADS=50
RESOLVER_TIMEOUT=5

# Proxy Configuration (optional)
# PROXY_URL=http://proxy.example.com:8080

# Rate Limits (requests per second)
RATE_LIMIT_VIRUSTOTAL=4
RATE_LIMIT_SECURITYTRAILS=1
RATE_LIMIT_SHODAN=1
RATE_LIMIT_CHAOS=10
RATE_LIMIT_GITHUB=1
RATE_LIMIT_NETLAS=1

# Sources to use (comma separated)
# Leave empty to use all available sources
ENABLED_SOURCES=virustotal,securitytrails,shodan,chaos,github,netlas

# Logging
LOG_LEVEL=info # Options: error, warn, info, debug, trace
VERBOSE=false